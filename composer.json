{
    "name": "lshamanl/symfony-ui-bundle-query",
    "description": "Symfony UI Bundle Query (Filters, Sorts, Pagination, One Resource)",
    "type": "symfony-bundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "alexander.erin",
            "email": "sanyaerin@mail.ru"
        }
